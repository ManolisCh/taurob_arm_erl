<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro"
       name="pg70" >

  <!-- common stuff -->
  <xacro:include filename="$(find taurob_schunk_lwa4d)/urdf/common.xacro" />
  <xacro:include filename="$(find taurob_schunk_lwa4d)/urdf/materials.urdf.xacro" />

  <!-- schunk pg70 -->
  <xacro:include filename="$(find taurob_schunk_lwa4d)/urdf/pg70/pg70.urdf.xacro" />

  <!-- Used for fixing robot to Gazebo -->
  <link name="world"/>

  <!-- arm -->
  <xacro:schunk_pg70 name="pg70" parent="world" has_podest="true">
    <origin xyz="0 0 0.026" rpy="0 0 0" /> <!--slightly above gazebo plane-->
  </xacro:schunk_pg70>

</robot>
